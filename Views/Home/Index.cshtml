@{
    ViewData["Title"] = "Home Page";
}
<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
<div id="form">
    <div id="buttons">
        <button id="local" onclick="submitlocal()">Lokalno</button>
        <button id="server">Na strežnik</button>
    </div>
    <form id="localform">
        <input type="text" id="imelocal" required>
        <input type="number" id="starostlocal" required>

    </form>
</div>
<table>
    <tr>
        <th>Ime</th>
        <th>Starost</th>
    </tr>
</table>


<script type="text/javascript" src="~/js/site.js"></script>
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
